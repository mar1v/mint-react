{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "es6"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@types/*": [
        "src/types/*"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "baseUrl": ".",
    "paths": {
      "@types/*": [
        "src/types/*"
      ],
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}